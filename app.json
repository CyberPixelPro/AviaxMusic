{
  "name": "SiestaXMusic",
  "description": "A Telegram Music Bot with proper functions written in Python with Pyrogram and Py-Tgcalls.",
  "repository": "https://github.com/TechShreyash/SiestaXMusic",
  "logo": "'https://telegra.ph/file/bc9c50e220e33c32fa84b.jpg",
  "keywords": [
  "python3",
  "telegram",
  "bot",
  "Siesta",
  "SiestaXMusic",
  "telegram-bot",
  "pyrogram"
  ],
  "repository": "https://github.com/TechShreyash/SiestaXMusic",
  "stack": "container",
  "env": {
    "API_ID": {
      "description": "Get this value from https://my.telegram.org",
      "value": "",
      "required": true
    },
    "API_HASH": {
      "description": "Get this value from https://my.telegram.org",
      "value": "",
      "required": true
    },
    "BOT_TOKEN": {
      "description": "A Bot's token from Botfather",
      "value": "",
      "required": true
    },
    "DURATION_LIMIT": {
      "description": "Duration Limit for Playout (In Mins).. Example: 60",
      "value": "60",
      "required": true
    },    
    "MONGO_DB_URI": {
      "description": "Mongo DB URL",
      "value": "",
      "required": true
    },
    "MUSIC_BOT_NAME": {
      "description": "A name for your Music Bot.",
      "value": "",
      "required": true
    },    
    "OWNER_ID": {
      "description": "The user id(s) of user(s) whom you would like to add as a OWNER. Multiple values shall be seperated with a space.",
      "value": "",
      "required": true
    },
    "STRING_SESSION": {
      "description": "A Pyrogram String Session.",
      "value": "",
      "required": true
    },
    "LOG_GROUP_ID": {
      "description": "Your Log Group ID, add your bot and promote as an admin with full rights!. Use only Group. Please don't use Channel ID.",
      "value": "",
      "required": true
    },
    "SUDO_USERS": {
      "description": "The user id(s) of user(s) whom you would like to add as a SUDO. Multiple values shall be seperated with a space.",
      "value": "",
      "required": true
    }
  },
  "buildpacks": [
        {
            "url": "heroku/python"
        },
        {
            "url": "heroku/nodejs"
        },
        {
            "url": "https://github.com/jonathanong/heroku-buildpack-ffmpeg-latest.git"
        }
    ],
    "formation": {
        "worker": {
            "quantity": 1,
            "size": "free"
        }
    },
    "stack": "container"
}